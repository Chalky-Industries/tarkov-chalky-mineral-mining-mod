	<!-- Chemistry Station Tool Module -->
		<!-- Added station tool module to the chemistry bench. The chemistry bench is where all the new ores are processed into elements and precious metals are refined. -->
		<!-- Ore processing station tool is required to break down ore/minerals-->
		<!-- Refining station tool is required to smelt precious metals into bars-->
		<!-- Chemistry Supplies slightly reduces craft times for all recipes made at this station-->
		
	<append xpath="/blocks/block[@name='chemistryStation']/property[@class='Workstation']/property[@name='Modules']/@value">,tools</append>
	<append xpath="/blocks/block[@name='chemistryStation']/property[@class='Workstation']">
		<property name="RequiredTools" value="toolOreProcessing,toolChemistrySupplies,toolMetalRefining"/>
		<property name="ToolNames" value="1,2,3"/>
	</append>
		
	<!-- Vehicle Salvage Changes -->
		<!-- Changed engine, battery, radiator, and acid from "drop on harvest" to "drop on destroy". -->
		<!-- Changed vanilla drop rates on engine, battery, radiator, and acid. Easier to acquire in the early game and tones down the excessive drop rate at end game-->
		<!-- Removed the perk magazine requirement to get acid from vehicles, tag="WasteTreasuresAcidHarvest"(doesn't exist in EFT overhaul). Lowered drop rate to compensate for adding a new way to acquire acid. -->			
		<!-- Changes were made on the following vehicle master blocks:
			cntCar03SedanDamage1Master
			cntSedan01White
			cntSedan3Wide01White
			cntFrontCrumpledSedan01White
			cntMinivan01White
			cntMinivan3Wide01White
			cntSUV01White
			cntPickupTruck01White
			cntPickupTruck3Wide01White
			cntSemiTruck01White
			cntPoliceCar01PickedLockBonus
			cntPoliceCar01AlarmUnlocked
			cntFireTruck01White
			cntBusSchool
			cntArmyTruckOpen
			cntFarmTruckClosed -->
	
	<remove xpath="/blocks/block[@name='cntCar03SedanDamage1Master']/drop[@name='smallEngine']" />
	<remove xpath="/blocks/block[@name='cntCar03SedanDamage1Master']/drop[@name='carBattery']" />
	<remove xpath="/blocks/block[@name='cntCar03SedanDamage1Master']/drop[@name='resourceRadiator']" />
	<remove xpath="/blocks/block[@name='cntCar03SedanDamage1Master']/drop[@name='resourceAcid']" />
	<insertAfter xpath="/blocks/block[@name='cntCar03SedanDamage1Master']/drop[@name='resourceScrapIron']">
		<drop event="Destroy" name="smallEngine" count="1" prob="0.175" />
		<drop event="Destroy" name="carBattery" count="1" prob="0.20" />
		<drop event="Destroy" name="resourceRadiator" count="1" prob="0.375" />
		<drop event="Destroy" name="resourceAcid" count="1" prob=".055" />
	</insertAfter>

